var isJpg = function (name) {
    return name.match(/jpg$/i);
};
var isJpeg = function (name) {
    return name.match(/jpeg$/i);
};
var isPng = function (name) {
    return name.match(/png$/i);
};



function saveOtp(otp,mobileNo){ 			 /* This function is used save student personal detail */     
	
	try{      
		     
		      var jsonObject={"mobile":mobileNo,"otp":otp};
		      $.ajax({
		          type: "POST",
				  url: "userRegistration/saveAndSendOtpOnMobile.html",
				  contentType: 'application/json',
				  data:JSON.stringify(jsonObject),
				  dataType: 'json',
		          success: function (data) {
		        	  setTimeout(function () {		
							displaySuccessMessages("errMsg", data.status,""); // function to display success message and it's appearance time 
						}, 1000);
		        	  clearSuccessMessageAfterTenSecond("errMsg");
		             },
		          error: function (data) {
		              alert("Error");
		          }
		      });
		
	  }catch(err){
		alert(err);
	}
}

