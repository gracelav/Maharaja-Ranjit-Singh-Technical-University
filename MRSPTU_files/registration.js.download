/*
 * -----------------------------------------------------------
 * function to to display success message and appearance time |
 * -----------------------------------------------------------
 */

function displaySuccessMessages(divId, dispStrongMessage, dispMessage) {  
	if (dispMessage == "" || dispMessage == null || dispMessage == undefined || dispMessage == "undefined") dispMessage = "";
	$("#" + divId).text("").append('<div><center><strong>' + dispStrongMessage + '!</strong><center>' + dispMessage + '</div>');
}

function clearSuccessMessageAfterTenSecond(MsgDivID) {
    setTimeout(function () {
        $("#" + MsgDivID).text("");    	
    }, 5000);
 
 }


function clearSuccessMessageAfterThreeSecond(MsgDivID) {
    setTimeout(function () {
        $("#" + MsgDivID).text("");    	
    }, 3000);
 
 }

function reloadForm(){     		  /* -- This function is use to reload form. -- */ 
	setTimeout(function(){ 		  /* -- Time out function. -- */
		location.reload()},5000)  /* -- Location.reload function is use to reload form on same page. -- */ 
}

function calculatePercentage(tot,obt,per,id)
{
	if($("#"+tot+id).val()!='' && $("#"+obt+id).val()!=''){
	var totMarks=parseInt($("#"+tot+id).val());
	var obtMarks=parseInt($("#"+obt+id).val());
	if(IsInteger($("#"+tot+id).get(0),'Total Marks') && IsInteger($("#"+obt+id).get(0),'Obtanied Marks')){
	if(obtMarks>totMarks){
		$("#"+obt+id).focus();
		 showerr($("#"+obt+id)[0], "Obtanied marks shouldn't be more than total marks!","block");
		 $("#"+obt+id).val('');
		 return false;
		 
	}else{
     var percentage=(obtMarks*100)/totMarks;
	     $("#"+per+id).val(percentage.toFixed(2));
	}
	}
	}else{
		$("#"+per+id).val('');
	}
    
}


