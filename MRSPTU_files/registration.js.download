 function hideAndShow(eligibity)
 {
	 if(eligibity=='10+')
	    {
	      $(".dpm").show();
	      $(".dpm1").show();
	      $(".2").hide();
	      $(".22").hide();
	      $(".23").hide();
	      $(".6th").show();
	    }
else if(eligibity=='12+')
 {
   $(".dpm").show();
   $(".dpm1").show();
   $(".6th").hide();
   $(".2").show();
   $(".22").show();
   $(".23").show();
 }
else if(eligibity=='Under')
 {
   $(".ug").show();
   $(".ug1").show();
 }
	 
 }

 
